			<input type="submit" class="btn" value="Submit Selections" id="submit-button"/>
		</div>
		
		<script>
			pop_up_shown = false;
			
			function continue_reg () {
				pop_up_shown = true;
				$("form.events").submit ();
			}
			
			$("form.events").submit (function () {
				var selected_ids = "";
				
				$("input[name='events[]']:checked").each (function () {
					selected_ids += $(this).val () + ",";
				});
			
				if (selected_ids != "" && pop_up_shown) {
					return true;
				}
			
				if (selected_ids != "") {
					$("#submit-button").attr ("value", "Loading....");
					
					$.ajax ({
						type: "POST",
						async: "false",
						url: "includes/get-event-conflicts.php",
						data: "ids=" + selected_ids,
						dataType: "html",
						success: function (response) {
							if (response.length == 0) {
								continue_reg ();
							} else if (response != "error") {
								$("#event-popup").dialog ({
									modal: true,
									minWidth: 500,
									maxWidth: 500,
									maxheight: 600
								});
								
								$(".popup-container").html (response);
								$(".popup-container").mCustomScrollbar({ scrollButtons: { enable: true }});
							} else {
								return true;
							}
							
							$("#submit-button").attr ("value", "Submit Selections");
							return false;
						}
					});
					
					return false;
				} else {
					alert ("Please select at least one event");
					return false;
				}
			});
		</script>
	</form>

				
				<script src="/50th/template/js/jquery-ui-1.10.2.js"></script>
				<script src="/50th/template/js/jquery.jqtransform.js.php"></script>
				<script src="/50th/template/js/jquery.tablesorter.js"></script>
				<script src="/50th/template/js/form-validation.js"></script>
				<script src="/50th/template/js/jquery.mCustomScrollbar.js"></script>
				
				<script>
					$(document).ready(function(){
						$("form").jqTransform();
					});
				</script>
			</div> <!-- #content-container -->
		</div> <!-- #content -->
	</body>
</html>
