<!-- 
function echeck(str){
	var at="@";
	var dot=".";
	var lat=str.indexOf(at);
	var lstr=str.length;
	var ldot=str.indexOf(dot);
		
	if (str.indexOf(at)==-1)
		return false;

	if (str.indexOf(at)==-1 || str.indexOf(at)==0 || str.indexOf(at)==lstr)
	   return false;

	if (str.indexOf(dot)==-1 || str.indexOf(dot)==0 || str.indexOf(dot)==lstr)
	    return false;

	 if (str.indexOf(at,(lat+1))!=-1)
	    return false;

	 if (str.substring(lat-1,lat)==dot || str.substring(lat+1,lat+2)==dot)
	    return false;

	 if (str.indexOf(dot,(lat+2))==-1)
	    return false;
		
	 if (str.indexOf(" ")!=-1)
	    return false;

	 return true;
} // end check e-mail address

// allow only numbers in the ext field
function numbersonly(myfield, e, dec){
	var key;
	var keychar;

	if (window.event)
   		key = window.event.keyCode;
	else if (e)
   		key = e.which;
	else
   		return true;
	keychar = String.fromCharCode(key);

	// control keys
	if ((key==null) || (key==0) || (key==8) || (key==9) || (key==13) || (key==27) )
   		return true;

	// numbers
	else if ((("0123456789").indexOf(keychar) > -1))
	return true;
	   

	// decimal point jump
	else if (dec && (keychar == ".")){
   		myfield.form.elements[dec].focus();
   	return false;
   	}
	else
   		return false;
} // end check for numbers - no strings allowed

// allow only Phone numbers and ext
function pnumbersonly(myfield, e, dec){
	var key;
	var keychar;

	if (window.event)
   		key = window.event.keyCode;
	else if (e)
   		key = e.which;
	else
   		return true;
	keychar = String.fromCharCode(key);

	// control keys
	if ((key==null) || (key==0) || (key==8) || (key==9) || (key==13) || (key==27) )
   		return true;

	// numbers
	else if ((("0123456789-()+ ext.EXT").indexOf(keychar) > -1))
	return true;
	   

	// decimal point jump
	else if (dec && (keychar == ".")){
   		myfield.form.elements[dec].focus();
   	return false;
   	}
	else
   		return false;
} // end check for numbers - no strings allowed
// -->
</script>

