<script>(function ($, Drupal, window, document, undefined) {
        $(document).ready(function() {
        jQuery(function($) {
          $("#tweet").tweet({
            modpath: "/sites/all/libraries/jquery.tweet/index.js.php",
            count: 5,
            loading_text: "loading twitter feed...",
            template: "{avatar} {text} {time} {reply_action} {retweet_action}",
            avatar_size: 0,
            page: 1,
            username: "uwindsor"
            /* etc... */
          }).ready(function(){
            var ul = $(this).find("ul.tweet_list");
            if (ul.children().length > 1) {
              var ticker = function() {
                setTimeout(function() {
                  var top = ul.position().top;
                  var h = ul.height();
                  var incr = (h / ul.children().length);
                  var newTop = top - incr;
                  if (h + newTop <= 10) newTop = 0;
                  ul.animate( {top: newTop}, 300 );
                  ticker();
                }, 5000);
              };
              ticker();
              var resizeTO = false;
              $(window).resize(function() {
                if(resizeTO !== false)
                clearTimeout(resizeTO);
                resizeTO = setTimeout(function(){$("ul.tweet_list").css("top", 0);}, 150);
              });
            }
          });
          
        });
        
        });})(jQuery, Drupal, this, this.document);</script>
