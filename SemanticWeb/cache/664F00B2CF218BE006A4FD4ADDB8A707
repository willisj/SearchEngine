<!--googleon: all--><!--googleoff: all--><div id="block-views-news-block_1" class="block block-views region-even even region-count-2 count-8 green_title law-library-news"><div class="block-inner">
          <h2 class="title">Law Library News</h2>
    <div class="content">
    <div class="view view-news view-id-news view-display-id-block_1 view-dom-id-1">
    
  
  
      <div class="view-content">
       <div class="">
